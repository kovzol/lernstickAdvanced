      <layout width="1024" height="600" y="200" dy="30" font="font_size_15.fnt" indention="30" description_x="170" description_y="320" textwrap="700"/>
      <layout width="800" height="600" y="200" dy="30" font="font_size_15.fnt" indention="30" description_x="110" description_y="320" textwrap="530"/>
    </config>
    <submenu icon="icon_config.jpg">
      <layout width="1366" height="768" y="210" dy="30" font="font_size_15.fnt" icon_inset="10" edit_x="10" edit_y="600" edit_width="1100" indention="30"/>
      <layout width="1280" height="1024" y="300" dy="40" font="font_size_20.fnt" icon_inset="10" edit_x="10" edit_y="830" edit_width="1050" indention="30"/>
      <layout width="1280" height="800" y="240" dy="30" font="font_size_15.fnt" icon_inset="10" edit_x="10" edit_y="620" edit_width="1050" indention="30"/>
      <layout width="1024" height="768" y="230" dy="30" font="font_size_15.fnt" icon_inset="10" edit_x="10" edit_y="600" edit_width="860" indention="30"/>
      <layout width="1024" height="600" y="170" dy="30" font="font_size_15.fnt" icon_inset="10" edit_x="10" edit_y="510" edit_width="860" indention="30"/>
      <layout width="800" height="600" y="155" dy="30" font="font_size_15.fnt" icon_inset="10" edit_x="10" edit_y="470" edit_width="640" indention="30"/>
      <text>More Configuration</text>
      <edit_prompt>Custom options:</edit_prompt>
      <back icon="icon_previous.jpg">Back to main menu</back>
      <config id="keyboard" icon="icon_keyboard.jpg">
        <layout width="1366" height="768" y="230" dy="30" font="font_size_15.fnt"/>
        <layout width="1280" height="1024" y="300" dy="40" font="font_size_20.fnt"/>
        <layout width="1280" height="800" y="230" dy="40" font="font_size_20.fnt"/>
        <layout width="1024" height="768" y="230" dy="30" font="font_size_15.fnt"/>
        <layout width="1024" height="600" y="170" dy="30" font="font_size_15.fnt"/>
        <layout width="800" height="600" y="170" dy="30" font="font_size_15.fnt"/>
      </config>
      <config id="live-media" icon="icon_storage_device.jpg" description_color="0x000000">
        <layout width="1366" height="768" y="230" dy="40" font="font_size_20.fnt" description_x="400" description_y="400"/>
        <layout width="1280" height="1024" y="300" dy="40" font="font_size_20.fnt" description_x="150" description_y="450"/>
        <layout width="1280" height="800" y="260" dy="40" font="font_size_20.fnt" description_x="150" description_y="430"/>
