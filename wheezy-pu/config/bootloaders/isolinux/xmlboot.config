      <layout width="1024" height="768" y="230" dy="30" font="font_size_15.fnt" indention="30" description_x="150" description_y="420" screenshot_x="150" screenshot_y="240"/>
      <layout width="1024" height="600" y="200" dy="30" font="font_size_15.fnt" indention="30" description_x="100" description_y="450" screenshot_x="100" screenshot_y="200"/>
      <layout width="800" height="600" y="200" dy="30" font="font_size_15.fnt" indention="30" description_x="90" description_y="390" screenshot_x="90" screenshot_y="210"/>
    </config>
    <config id="persistence" icon="icon_partition.jpg">
      <layout width="1366" height="768" y="230" dy="40" font="font_size_20.fnt" indention="30" description_x="220" description_y="400" textwrap="1000"/>
      <layout width="1280" height="1024" y="300" dy="40" font="font_size_20.fnt" indention="30" description_x="170" description_y="500" textwrap="800"/>
      <layout width="1280" height="800" y="280" dy="40" font="font_size_20.fnt" indention="30" description_x="170" description_y="420" textwrap="800"/>
