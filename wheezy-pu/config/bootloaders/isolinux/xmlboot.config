        <layout width="800" height="600" y="200" dy="30" font="font_size_15.fnt" description_x="90" description_y="320"/>
      </config>
      <option id="swap_partition"/>
      <option id="lapic_timer"/>
      <option id="dynamic_ticks"/>
      <option id="pae"/>
      <option id="messages"/>
      <option id="debug"/>
    </submenu>
    <start icon="icon_integrity_check.jpg" index="0" label="linux1 initrd=/live/initrd1.img boot=live live-media-timeout=10 verify-checksums">
      <text>Check integrity of boot medium</text>
      <splash>splash_linux.jpg</splash>
    </start>
  </menu>
</xmlboot>
